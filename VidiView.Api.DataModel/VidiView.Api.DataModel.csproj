<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<RootNamespace>VidiView.Api</RootNamespace>
	<Platforms>AnyCPU</Platforms>
    <Company>Distributed Medical AB</Company>
	<Title>VidiView OpenAPI Data Model</Title>
	<Product>VidiView® Enterprise Imaging</Product>
	<Copyright>Copyright © 2023-2024 Distributed Medical AB, http://www.distributedmedical.com. All rights reserved</Copyright>
	<AssemblyVersion>5.0</AssemblyVersion>
	<FileVersion>5.0</FileVersion>
	<Authors>Distributed Medical AB</Authors>
	<Description>VidiView data model objects for accessing VidiView API</Description>
	<PackageTags>vidiview</PackageTags>
	<Version>5.0.2</Version>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<PackageProjectUrl>https://distributedmedical.com/vidiview/</PackageProjectUrl>
	<RepositoryUrl>https://github.com/distributed-medical/VidiView_OpenAPI</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="README.md" Pack="true" PackagePath="" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NuGet.CommandLine" Version="6.8.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
